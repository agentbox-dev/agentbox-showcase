# Production Environment Variables
# This file is used for production deployment

# API Configuration
API_URL=https://api.agentbox.lingyiwanwu.com

# Next.js Configuration
NEXT_PUBLIC_APP_URL=https://agentbox.lingyiwanwu.com
NEXT_PUBLIC_API_URL=https://api.agentbox.lingyiwanwu.com

# Authentication
NEXTAUTH_URL=https://agentbox.lingyiwanwu.com
NEXTAUTH_SECRET=your-production-secret-key-here

# Database (if needed)
# DATABASE_URL=your-production-database-url

# External Services
# GOOGLE_CLIENT_ID=your-production-google-client-id
# GOOGLE_CLIENT_SECRET=your-production-google-client-secret

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_DEBUG_MODE=false

# Performance
NEXT_PUBLIC_ENABLE_SW=false
